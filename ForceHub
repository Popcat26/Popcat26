-- Scripts:

local function EHKAXY_fake_script() -- OnOffantiVoid.LocalScript 
	local script = Instance.new('LocalScript', OnOffantiVoid)

	script.Parent.MouseButton1Click:Connect(function()
		local A = Instance.new("Message", workspace)
		A.Text = "ForceHubX - Slap Battles : ðŸ”´"
		wait(5)
		local B = Instance.new("Part", workspace.Arena["main island"])
		B.Anchored = true
		B.Position = Vector3.new(0,-8.5,0)
		B.Size = Vector3.new(2048,1,2048)
		B.Transparency = 0.5
		B.Touched:Connect(function()
			while wait() do
				game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 70
			end
		end)
		A.Text = "ForceHubX - Slap Battles : ðŸŸ¢"
		wait(5)
		A:Destroy()
	end)
end
coroutine.wrap(EHKAXY_fake_script)()
